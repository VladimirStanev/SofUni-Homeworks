using System;

namespace _07._Water_Overflow
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int numberOfPourrings = int.Parse(Console.ReadLine());
            int capacity = 255;
            int litresInTank = 0;
            int pourredInAbsLittres = 0;

            for(int i = 0; i < numberOfPourrings; i++)
            {               
                int newPour = int.Parse(Console.ReadLine());
                litresInTank += newPour;
                if(capacity < litresInTank)
                {
                    litresInTank -= newPour;
                    Console.WriteLine("Insufficient capacity!");
                    continue;
                }
                pourredInAbsLittres += newPour;
            }
            Console.WriteLine(pourredInAbsLittres);
        }
    }
}
